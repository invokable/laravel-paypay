{
    "name": "revolution/laravel-paypay",
    "description": "Simple integration Laravel and PayPay API",
    "keywords": [
        "laravel",
        "paypay"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0||^12.0",
        "paypayopa/php-sdk": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "laravel/pint": "^1.22"
    },
    "autoload": {
        "psr-4": {
            "Revolution\\PayPay\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "kawax",
            "email": "kawaxbiz@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Revolution\\PayPay\\PayPayServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
